{
  "name": "tanstarter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check": "pnpm format && pnpm lint && pnpm check-types",
    "deps": "pnpm dlx taze@latest -Ilw",
    "deps:major": "pnpm dlx taze@latest major -Ilw",
    "ui": "pnpm dlx shadcn@latest",
    "gen:client": "encore gen client iptvtest-gibi --output=./src/lib/encore-client.ts --env=local",
    "gen:client:staging": "encore gen client iptvtest-gibi --output=./src/lib/encore-client.ts --env=staging",
    "gen:client:prod": "encore gen client iptvtest-gibi --output=./src/lib/encore-client.ts --env=production",
    "deploy:netlify": "pnpm build:spa && echo 'Ready for Netlify deployment. Upload dist/ folder or connect via Git.'",
    "deploy:vercel": "pnpm build:spa && echo 'Ready for Vercel deployment. Upload dist/ folder or connect via Git.'",
    "deploy:cloudflare": "pnpm build:spa && echo 'Ready for Cloudflare Pages deployment. Upload dist/ folder or connect via Git.'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "@tanstack/react-router-with-query": "^1.130.12",
    "@tanstack/react-start": "^1.130.15",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "libpgs": "^0.8.1",
    "lucide-react": "latest",
    "mediainfo.js": "^0.3.5",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-infinite-scroll-hook": "^6.0.1",
    "shaka-player": "^4.15.9",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.0.6",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/eslint-plugin-router": "^1.130.12",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "babel-plugin-react-compiler": "latest",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "rc",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
